# Firebase Cloud Messaging Configuration
# Get these values from Firebase Console -> Project Settings -> General -> Your apps -> Web app

# Firebase API Key
VITE_FIREBASE_API_KEY=your_api_key_here

# Firebase Auth Domain (format: project-id.firebaseapp.com)
VITE_FIREBASE_AUTH_DOMAIN=hotel-mate-d878f.firebaseapp.com

# Firebase Project ID
VITE_FIREBASE_PROJECT_ID=hotel-mate-d878f

# Firebase Storage Bucket (format: project-id.appspot.com)
VITE_FIREBASE_STORAGE_BUCKET=hotel-mate-d878f.appspot.com

# Firebase Messaging Sender ID
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here

# Firebase App ID
VITE_FIREBASE_APP_ID=your_app_id_here

# VAPID Key for Web Push Notifications
# Get this from Firebase Console -> Project Settings -> Cloud Messaging -> Web Push certificates
VITE_FIREBASE_VAPID_KEY=your_vapid_key_here

# API Base URL
VITE_API_BASE_URL=http://localhost:8000
